{
	"name": "@das.laboratory/nps-i",
	"version": "2.2.2",
	"description": "Interactive mode for nps",
	"license": "MIT",
	"repository": "git@github.com:NicolasReibnitz/nps-i.git",
	"author": "siddharthkp",
	"main": "index.js",
	"bin": {
		"nps-i": "index.js",
		"ni": "index.js"
	},
	"scripts": {
		"bump:major": "npm version major -f -m \"[v%s] $npm_config_message\"",
		"bump:minor": "npm version minor -f -m \"[v%s] $npm_config_message\"",
		"bump:patch": "npm version patch -f -m \"[v%s] $npm_config_message\"",
		"postversion": "git push && git push --tags && npm publish",
		"preversion": "git add .",
		"version": "git add ."
	},
	"files": [
		"examples",
		"exec.js"
	],
	"keywords": [],
	"dependencies": {
		"execa": "^1.0.0",
		"fuzzy": "0.1.3",
		"inquirer": "8.1.5",
		"inquirer-autocomplete-prompt": "1.4.0",
		"nps": "5.10.0",
		"nps-utils": "1.7.0",
		"right-pad": "1.0.1"
	}
}
